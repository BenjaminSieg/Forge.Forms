<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:routes="clr-namespace:Forge.Forms.Demo.Routes"
                    xmlns:views="clr-namespace:Forge.Forms.Demo.Views">

    <DataTemplate DataType="{x:Type routes:HomeRoute}">
        <views:FormView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type routes:ExamplesRoute}">
        <views:ExamplesView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type routes:XmlExamplesRoute}">
        <views:XmlExamplesView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type routes:CsharpExamplesRoute}">
        <views:CsharpExamplesView />
    </DataTemplate>

</ResourceDictionary>